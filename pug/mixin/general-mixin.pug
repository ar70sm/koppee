mixin ulArr (arr)
  ul
    each i in arr
      if (typeof i)=="object"
        li.dropdown=i.title+" "
          i.fa-solid.fa-caret-down
          +ulArr(i.ele)
      else
        if (i == "Home")
          if(i == CurrentPage)
            li.active: a(href="index.html")=i
          else 
            li: a(href="index.html")=i
        else
          if(i == CurrentPage)
            li.active: a(href=i.toLowerCase()+".html")=i
          else 
            li: a(href=i.toLowerCase()+".html")=i

